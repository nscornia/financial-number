{
  "name": "financial-number",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build",
    "fix:eslint": "eslint --fix './**/*.{js,ts}'",
    "fix:prettier": "prettier --write './**/*.{js,json,ts,md,html,css,scss}'",
    "format": "run-s fix:*",
    "lint": "run-s lint:*",
    "lint:format": "prettier --check './**/*.{js,jsx,json,ts,tsx,md,html,css,scss}'",
    "lint:js": "eslint --cache --quiet \"**/*.{js,ts}\"",
    "lint:ng": "ng lint",
    "lint:spelling": "cspell \"./src/**/*.{js,ts,scss,css}\" --no-progress",
    "lint:styles": "stylelint --fix --ignore-path .gitignore \"src/**/*.{scss,css}\"",
    "ng": "ng",
    "prepare": "husky",
    "start": "ng serve",
    "test": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js -c=jest.config.mjs --verbose --no-cache",
    "test-ci": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js -c=jest.config.mjs --verbose --no-cache --runInBand --detectOpenHandles --forceExit",
    "test-watch": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js -c=jest.config.mjs --verbose --no-cache --watch",
    "watch": "ng build --watch --configuration development"
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/builder": "^17.2.1",
    "@angular-eslint/eslint-plugin": "^17.2.1",
    "@angular-eslint/eslint-plugin-template": "^17.2.1",
    "@angular-eslint/schematics": "^17.2.1",
    "@angular-eslint/template-parser": "^17.2.1",
    "@angular/animations": "^17.2.3",
    "@angular/cdk": "^17.2.1",
    "@angular/common": "^17.2.3",
    "@angular/compiler": "^17.2.3",
    "@angular/core": "^17.2.3",
    "@angular/forms": "^17.2.3",
    "@angular/material": "^17.2.1",
    "@angular/platform-browser": "^17.2.3",
    "@angular/platform-browser-dynamic": "^17.2.3",
    "@angular/router": "^17.2.3",
    "lodash-es": "^4.17.21",
    "rxjs": "~7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "^17.0.2",
    "@angular-devkit/build-angular": "^17.2.2",
    "@angular/cli": "^17.2.2",
    "@angular/compiler-cli": "^17.2.3",
    "@types/jest": "^29.5.12",
    "@types/lodash-es": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "babel-jest": "^29.7.0",
    "commitizen": "^4.3.0",
    "cspell": "^8.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^9.0.11",
    "jasmine-core": "~5.1.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "^14.0.3",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "testdouble": "^3.20.1",
    "ts-jest": "^29.1.2",
    "typescript": "~5.3.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,json,jsx,ts,tsx,yml,html,css,scss}": "prettier --write",
    "*.{js,jsx,ts,tsx,html}": "eslint --fix",
    "*.{scss,css}": "stylelint --fix --ignore-path .gitignore"
  }
}
